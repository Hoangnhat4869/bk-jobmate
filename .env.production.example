# Production Environment Variables
# Copy this file to .env for production builds

# API Configuration - Production
EXPO_PUBLIC_BASE_URL=https://api.cyese.me/
EXPO_PUBLIC_API_URL=https://api.cyese.me/
EXPO_PUBLIC_ENVIRONMENT=production

# Google OAuth Client IDs (Production)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_prod_web_client_id_here
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_prod_android_client_id_here
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_prod_ios_client_id_here

# App Configuration
EXPO_PUBLIC_APP_SCHEME=com.bkjobmate

# Production Settings
EXPO_PUBLIC_DEV_MODE=false
EXPO_PUBLIC_DEBUG_API=false

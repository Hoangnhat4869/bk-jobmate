# Environment Variables for BKJobmate
# Copy this file to .env and fill in your actual values

# API Configuration
# Use one of the following based on your environment:
# Development (local): http://localhost:3000/api/
# Staging: https://staging-api.cyese.me/
# Production: https://api.cyese.me/
EXPO_PUBLIC_BASE_URL=https://api.cyese.me/
EXPO_PUBLIC_API_URL=https://api.cyese.me/

# Google OAuth Client IDs
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_web_client_id_here
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_android_client_id_here
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_ios_client_id_here

# App Configuration
EXPO_PUBLIC_APP_SCHEME=com.bkjobmate

# Development Settings
EXPO_PUBLIC_DEV_MODE=true
EXPO_PUBLIC_DEBUG_API=false

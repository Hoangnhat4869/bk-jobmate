# Local Development Environment Variables
# Copy this file to .env for local development

# API Configuration - Local Development
EXPO_PUBLIC_BASE_URL=http://localhost:3000/api/
EXPO_PUBLIC_API_URL=http://localhost:3000/api/
EXPO_PUBLIC_ENVIRONMENT=development

# Google OAuth Client IDs (Development)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_dev_web_client_id_here
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_dev_android_client_id_here
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_dev_ios_client_id_here

# App Configuration
EXPO_PUBLIC_APP_SCHEME=com.bkjobmate

# Development Settings
EXPO_PUBLIC_DEV_MODE=true
EXPO_PUBLIC_DEBUG_API=true

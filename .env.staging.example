# Staging Environment Variables
# Copy this file to .env for staging builds

# API Configuration - Staging
EXPO_PUBLIC_BASE_URL=https://staging-api.cyese.me/
EXPO_PUBLIC_API_URL=https://staging-api.cyese.me/
EXPO_PUBLIC_ENVIRONMENT=staging

# Google OAuth Client IDs (Staging)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_staging_web_client_id_here
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_staging_android_client_id_here
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_staging_ios_client_id_here

# App Configuration
EXPO_PUBLIC_APP_SCHEME=com.bkjobmate

# Staging Settings
EXPO_PUBLIC_DEV_MODE=false
EXPO_PUBLIC_DEBUG_API=true
